            private TextWatcher tw = new TextWatcher() {
        @Override
        public void beforeTextChanged(CharSequence s, int start, int count, int after) {

        }


        @Override
        public void onTextChanged(CharSequence s, int start, int before, int count) {
            Button button_add = findViewById(R.id.btadd);
            final int[] ac1 = new int[1];
            final int[] ac2 = new int[1];
            final int[] ac3 = new int[1];
            final int[] ac4 = new int[1];

            TextView o1=findViewById(R.id.tv);
            TextView o2=findViewById(R.id.tv2);
            TextView o3=findViewById(R.id.tv3);
            TextView o4=findViewById(R.id.tv4);
            ac1[0]= Integer.parseInt(o1.getText().toString());
            ac2[0]= Integer.parseInt(o2.getText().toString());
            ac3[0]= Integer.parseInt(o3.getText().toString());
            ac4[0]= Integer.parseInt(o4.getText().toString());
            button_add.setEnabled(ac1[0]!=0&&ac2[0]!=0&&ac3[0]!=0&&ac4[0]!=0);
        }

        @Override
        public void afterTextChanged(Editable s) {

        }


    };
